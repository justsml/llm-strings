{
  "name": "llm-strings",
  "version": "1.1.0",
  "description": "Parse and build LLM connection strings â€” like database URLs, but for LLM APIs",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./parse": {
      "import": {
        "types": "./dist/parse.d.ts",
        "default": "./dist/parse.js"
      },
      "require": {
        "types": "./dist/parse.d.cts",
        "default": "./dist/parse.cjs"
      }
    },
    "./normalize": {
      "import": {
        "types": "./dist/normalize.d.ts",
        "default": "./dist/normalize.js"
      },
      "require": {
        "types": "./dist/normalize.d.cts",
        "default": "./dist/normalize.cjs"
      }
    },
    "./validate": {
      "import": {
        "types": "./dist/validate.d.ts",
        "default": "./dist/validate.js"
      },
      "require": {
        "types": "./dist/validate.d.cts",
        "default": "./dist/validate.cjs"
      }
    },
    "./providers": {
      "import": {
        "types": "./dist/providers.d.ts",
        "default": "./dist/providers.js"
      },
      "require": {
        "types": "./dist/providers.d.cts",
        "default": "./dist/providers.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "keywords": [
    "llm",
    "connection-string",
    "openai",
    "anthropic",
    "ai",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justsml/llm-strings.git"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
